@require "~styl/resources.styl"

#app
  margin-top 60px
  color $pickled-bluewood
  font-family "Avenir", Helvetica, Arial, sans-serif
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
  text-align center

h1,
h2
  font-weight normal

ul
  padding 0
  list-style-type none

li
  display inline-block
  margin 0 10px

a
  color $ocean-green
